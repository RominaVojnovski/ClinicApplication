<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:ns0="http://www.example.org/clinic/schemas/treatment" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.example.org/clinic/schemas/treatment" elementFormDefault="qualified">
   <xsd:complexType name="patientDTO">
      <xsd:sequence>
         <xsd:element name="id" type="xsd:long"/>
         <xsd:element name="patient-id" type="xsd:long"/>
         <xsd:element name="name" type="xsd:string"/>
         <xsd:element name="dob" type="xsd:dateTime"/>
         <xsd:element name="age" type="xsd:long"/>
         <xsd:element name="treatments" type="xsd:long" maxOccurs="unbounded" nillable="true"/>
      </xsd:sequence>
   </xsd:complexType>
</xsd:schema>
